{
  "name": "voice-office-chatbot",
  "version": "1.0.0",
  "description": "Intermediate voice-enabled office assistant chatbot",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "mobile": "cd mobile && npm start",
    "mobile:android": "cd mobile && expo run:android",
    "mobile:ios": "cd mobile && expo run:ios",
    "mobile:web": "cd mobile && expo start --web",
    "install-all": "npm install && cd mobile && npm install",
    "test": "node server/config/database.js",
    "test-db": "node server/config/database.js",
    "test-connection": "node -e \"require('./server/config/database').getConnection().then(() => console.log('✅ Connection successful')).catch(err => console.error('❌ Connection failed:', err.message))\"",
    "setup": "npm install && cd mobile && npm install && echo 'Setup complete!'",
    "clean": "rm -rf node_modules mobile/node_modules && npm run setup",
    "lint": "echo 'Linting not configured yet'",
    "build": "echo 'Build process not configured yet'",
    "postinstall": "echo 'Post-install tasks completed'"
  },
